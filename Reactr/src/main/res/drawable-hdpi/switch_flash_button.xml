<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"
          android:drawable="@drawable/rsz_camera_flash_on_black" />
    <item android:state_checked="false"
          android:drawable="@drawable/rsz_camera_flash_off_black" />

    <item android:state_enabled="false"
          android:drawable="@drawable/rsz_camera_flash_off_notclickable" />
    <item android:state_pressed="true"
          android:drawable="@drawable/rsz_camera_flash_on_white" />
</selector>